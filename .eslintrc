{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
 "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-curly-spacing": [2, "always"],
    "jsx-a11y/no-static-element-interactions": "off",
    "no-plusplus": "off"
  }
}
